{
  "enabled": true,
  "deduplication": {
    "enabled": true,
    "allowedTools": ["Read", "Grep", "Glob"],
    "protectedTools": [
      "Task",
      "TodoWrite",
      "TodoRead",
      "AskUserQuestion",
      "Skill",
      "SlashCommand",
      "EnterPlanMode",
      "ExitPlanMode"
    ]
  },
  "tokenEstimation": {
    "Read": 1000,
    "Grep": 400,
    "Glob": 150
  },
  "output": {
    "style": "dcp",
    "prefix": "[DCP]",
    "showTokenEstimates": true,
    "showPerToolBreakdown": true
  },
  "limits": {
    "maxTranscriptSize": 52428800,
    "timeoutSeconds": 5
  }
}
